<%- include("Partials/header"); -%>

<body class="contact-body">
    <div class="container">
        <h1>Feedback Form</h1>
        <div class="row">
            <div class="col-lg-6 col-md-12">
                <form method="post">
                    <h3>Name</h3>
                    <input type="name" class="form-control" name="name" style="width: 50%;">
                    <h3>Email</h3>
                    <input type="email" class="form-control" name="email" style="width: 50%;">
                    <h3>Feedback</h3>
                    <textarea class="form-control" name="content" rows="4" cols="50"></textarea>
                    <button type="submit" class="btn btn-primary feedback-btn"
                        style=" margin-top: 20px; margin-bottom: 10px;">Submit</button>
                </form>
            </div>
        </div>
<%- include("Partials/footer"); -%>